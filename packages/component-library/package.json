{
  "name": "component-library",
  "version": "0.1.0",
  "description": "Library for theme-able, accessible, progressively-enhancing react components",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:copy-files && yarn build:types",
    "build:cjs": "node ../../scripts/build cjs",
    "build:esm": "node ../../scripts/build esm",
    "build:es": "node ../../scripts/build es",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@types/styled-components": "^5.1.4",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@types/react": "^16.8.6",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
